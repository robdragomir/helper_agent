# Google Gemini API Key
GOOGLE_API_KEY=your_google_api_key_here

# Tavily API Key for web search
TAVILY_API_KEY=your_tavily_api_key_here

# LLM Configuration
LLM_MODEL=gemini-pro
EMBEDDING_MODEL=all-MiniLM-L6-v2

# Knowledge Base Configuration
LANGGRAPH_DOCS_URL=https://langchain-ai.github.io/langgraph/reference/langgraph_docs.txt
LOCAL_DOCS_PATH=data/langgraph-docs.txt
KB_CHUNKS_PATH=data/kb_chunks.json
VECTOR_STORE_PATH=data/faiss_index

# RAG Configuration
TOP_K_CHUNKS=5
SIMILARITY_THRESHOLD=0.3

# Search Configuration
MAX_ONLINE_SEARCH_RESULTS=5
MIN_SOURCE_AGREEMENT_RATIO=0.6

# Telemetry Configuration
TELEMETRY_LOG_PATH=logs/telemetry.jsonl
EVAL_METRICS_PATH=logs/eval_metrics.json

# Model Configuration
CHUNK_SIZE_FOR_AGENTIC_CHUNKING=2000